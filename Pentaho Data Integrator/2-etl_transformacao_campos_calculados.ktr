    <quoteString>N</quoteString>
    <sql>-- Atualiza o nome do setor de acordo com o id do serviço

-- GST: 664 - 837
-- GSI: 840 - 871 / 873 - 966 / 971 - 975 / 977 - 986 / 1047 - 1079 / 1116 - 1176 / 1178 - 1305 / 1310 / 1322 - 1362 / 1367 - 1416 / 1433 - 1502 
-- CDI: 1020 / 967 - 970 / 976 / 987 - 1046 / 1080 - 1115 / 1177 / 1306 - 1309 / 1313 - 1321 / 1363 - 1366 / 1417 - 1432
-- Setor não Informado: -1
 
-- GST
update public.st_tickets 
   set nm_setor = 'GST'
 where service_id between 664 and 837;

-- GSI
update public.st_tickets 
   set nm_setor = 'GSI'
 where service_id between 840 and 871
    or service_id between 873 and 966
    or service_id between 971 and 975
    or service_id between 977 and 986
    or service_id between 1047 and 1079
    or service_id between 1116 and 1176
    or service_id between 1178 and 1305
    or service_id = 1310 
    or service_id between 1322 and 1362
    or service_id between 1367 and 1416
    or service_id between 1433 and 1502
    or service_id = 1576
    or service_id = 1601;      
    
-- CDI
update public.st_tickets 
   set nm_setor = 'CDI'
 where service_id = 1020 
    or service_id between 967 and 970
    or service_id = 976 
    or service_id between 987 and 1046
    or service_id between 1080 and 1115
    or service_id = 1177
    or service_id between 1306 and 1309     
    or service_id between 1313 and 1321
    or service_id between 1363 and 1366
    or service_id between 1417 and 1432;

-- Setor não Informado
update public.st_tickets 
   set nm_setor = 'N/A'
 where service_id = -1;</sql>
