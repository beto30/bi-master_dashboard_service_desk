    <quoteString>N</quoteString>
    <sql>-- Atualiza o nome de todos os serviços que não forem null (-1). Os demais já foram tratados anteriormente (Step Trata Serviços Nulos)

update st_tickets TICKETS
   set nm_servico = (select SERVICO.name
 					   from st_service  SERVICO 
     				  where SERVICO.id = TICKETS.service_id
     				  limit 1);

update st_tickets 
   set nm_servico = 'Serviço não informado'
 where service_id = -1</sql>
